{
  "name": "vue-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "run-p test:unit",
    "test:unit": "vitest --environment jsdom --root src/",
    "lint": "run-p lint:code lint:commits",
    "lint:code": "run-p type-check lint-js format-other",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint-js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format-other": "prettier --write '**/*.(html|css|json|y?(a)ml|md)'",
    "lint:commits": "commitlint --from=$(git merge-base --fork-point main)"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuetify": "^3.5.8"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.8.0",
    "@mdi/js": "^7.3.67",
    "@rushstack/eslint-patch": "^1.1.4",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.3",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@types/jsdom": "^21.0.0",
    "@types/node": "20.17.43",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0-beta.3",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "commitlint": "19.8.0",
    "eslint": "8.56.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "9.33.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.0.0",
    "prettier": "^3.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "semantic-release": "^23.0.0",
    "typescript": "~4.9.0",
    "vite": "^5.0.0",
    "vite-plugin-vuetify": "^2.0.0",
    "vitest": "^1.0.0-beta.3",
    "vue-tsc": "^2.0.0"
  }
}
